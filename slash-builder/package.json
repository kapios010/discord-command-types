{
    "name": "@dsc-slash/builder",
    "version": "0.0.0",
    "author": {
        "name": "kapios010",
        "url": "https://kapios.dev"
    },
    "scripts": {
        "build": "tsup src/**/* --dts",
        "dev": "tsup src/**/* --watch",
        "test": "vitest --run",
        "test:watch": "vitest"
    },
    "exports": {
        "./options/choices": {
            "default": "./dist/options/choices/index.js",
            "types": "./dist/options/choices/index.d.ts",
            "module": "./dist/options/choices/index.mjs"
        },
        "./options/*": {
            "default": "./dist/options/*.js",
            "types": "./dist/options/*.d.ts",
            "module": "./dist/options/*.mjs"
        },
        "./command/data": {
            "default": "./dist/command/data.js",
            "types": "./dist/command/data.d.ts",
            "module": "./dist/command/data.mjs"
        },
        "./command/options_parser": {
            "default": "./dist/command/options_parser.js",
            "types": "./dist/command/options_parser.d.ts",
            "module": "./dist/command/options_parser.mjs"
        },
        "./command": {
            "default": "./dist/command/index.js",
            "types": "./dist/command/index.d.ts",
            "module": "./dist/command/index.mjs"
        },
        ".": {
            "default": "./dist/index.js",
            "types": "./dist/index.d.ts",
            "module": "./dist/**/*.mjs"
        }
    },
    "dependencies": {
        "@sapphire/shapeshift": "^4.0.0",
        "discord.js": "^14.21.0",
        "lilconfig": "^3.1.3",
        "parent-module": "^3.1.0"
    },
    "devDependencies": {
        "@types/node": "^24.3.0",
        "tsup": "^8.5.0",
        "vitest": "^3.2.4"
    }
}
